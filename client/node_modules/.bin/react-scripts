#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules/react-scripts/bin/node_modules:/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules/react-scripts/node_modules:/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules:/root/JyotiCables/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules/react-scripts/bin/node_modules:/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules/react-scripts/node_modules:/root/JyotiCables/client/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.24.7_@babel+plugin-transform-react-jsx@7.24.7_76svw2iqjx6o4fgosxx3hnypwy/node_modules:/root/JyotiCables/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
